<ContentDialog
    x:Class="TFS.Client.Dialogs.SelectTeamProjectCollectionDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TFS.Client.Dialogs"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    x:Uid="SelectTeamProjectCollectionDialog"
    x:Name="View"
    PrimaryButtonText=""
    SecondaryButtonText=""
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick"
    Loaded="View_Loaded">

    <Grid>
        <TextBox Text="{Binding ViewModel.Collection, ElementName=View, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" InputScope="Url" />
    </Grid>
</ContentDialog>
